{
    "name": "main",
    "version": "0.1.0",
    "private": true,
    "homepage": "/main",
    "proxy": "http://agregator.test/",
    "dependencies": {
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-scripts": "1.0.17"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",

        "postbuild": "yarn copyindex && yarn movebuild",
        "copyindex": "cp build/index.html ../../public/index.html",
        "movebuild": "rm -rf ../../public/main && mv build ../../public/main"
    }
}
